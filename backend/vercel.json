{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "vercel.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "vercel.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}