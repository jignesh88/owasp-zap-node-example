{
  "version": 2,
  "functions": {
    "vercel.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}